
<div class="page-header">
  <h1>Users
    <small>
      <div class="pull-right">
        <%= link_to 'Create user', new_user_path, :class => "btn btn-info" %>
      </div>
    </small>
  </h1>
</div>

<table class="table">
  <thead>
    <tr>
      <th>First name</th>
      <th>Last name</th>
      <th>Email</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.first_name %></td>
        <td><%= user.last_name %></td>
        <td><%= user.email %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
